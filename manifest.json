{
    "name":"ColorCatcher",
    "description":"A color picker extinsion",
    "version":"1.0",
    "manifest_version":3,
    "host_permissions":["<all_urls>"],
    "permissions":[
      "activeTab","scripting","tabs"
    ],
    "action":{
      "default_popup":"popup.html"
    },
    "icons":{
        "16":"/images/CC_16.png",
        "32":"/images/CC_32.png",
        "48":"/images/CC_48.png",
        "128":"/images/CC_128.png"
    }

}