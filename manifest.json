{
    "name":"ColorCatcher",
    "description":"A color picker extinsion",
    "version":"1.0",
    "manifest_version":3,
    "host_permissions":["<all_urls>"],
    "permissions":[
      "activeTab","scripting","tabs"
    ],
    "action":{
      "default_popup":"popup.html"
    },
    "icons":{
        "16":"/images/picker_16.png",
        "32":"/images/picker_32.png",
        "48":"/images/picker_48.png"
    }

}